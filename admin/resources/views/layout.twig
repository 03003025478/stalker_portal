<!DOCTYPE html>
<html lang="en">
    <head>
        {% block head %}
            <meta charset="utf-8">
            <title>{{title}}</title>
            {% include "basic_templates/meta.twig" %}
            {% include "basic_templates/css.twig" %}
        {% endblock head %}
		<link href='http://fonts.googleapis.com/css?family=PT+Sans:400,700,400italic,700italic&subset=latin,cyrillic-ext,latin-ext,cyrillic' rel='stylesheet' type='text/css'>
    </head>
    <body>
        {% block container %}
            {% block header %}
                {% include "basic_templates/header.twig" %}
            {% endblock header %}

            {% block maincontainer %}
                <!--Start Container-->
				<div id="wrapper">
                <div id="main" class="container-fluid">
                    <div class="row">
                        {% block sidebar %}
                            {% include "basic_templates/sidebar.twig" %}
                        {% endblock sidebar %}
                        <!--Start Content-->
						<div id="main-wrapper" >
                        <div id="content" >
                            <div class="preloader">
                                <img src="img/devoops_getdata.gif" class="devoops-getdata" alt="preloader"/>
                            </div>
                            <div id="ajax-content">
                                <div class="row">
                                    <div class="box-content">
                                        <div id="breadcrumb">
                                             {{ renderBreadCrumbs() }}
                                        </div>
                                    </div>
                                </div>
                                
                                {% block content %}
                                {% endblock content %}
                            </div>
                        </div>
                        </div>
                        <!--End Content-->
                 
                </div></div>
                <!--End Container--></div>
            {% endblock maincontainer %}

            {% block footer %}
                {% include "basic_templates/footer_js.twig" %}
            {% endblock footer %}
        {% endblock container %}
    </body>
</html>